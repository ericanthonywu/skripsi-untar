<script src="<%= BASE_URL %>admin/js/lib/bootstrap-select.js"></script>

<script src="<%= BASE_URL %>admin/js/globalsetup.js"></script>
<script src="<%= BASE_URL %>admin/js/custom_script.js"></script>
<script src="<%= BASE_URL %>admin/js/lib/chosen-select.js"></script>
<script src="<%= BASE_URL %>admin/js/lib/datepicker.js"></script>

<script src="<%= BASE_URL %>admin/js/login.js"></script>

</body>

</html>